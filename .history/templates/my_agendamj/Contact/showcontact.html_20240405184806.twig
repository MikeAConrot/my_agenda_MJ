{% block title_name %}
    CONTACTOS
{% endblock %}
{% block header %}
    <h1>PHON</h1>
{% endblock %}
 
{% block content  %}
  <div class="container"> <h1 class="text-center">Contactos</h1> <table class="table table-striped table-bordered"> <thead> <tr> <th>Nombre Completo</th> <th>Correo</th> <th>Acciones</th> </tr> </thead> <tbody> {% for contac in contacts %} <tr> <td>{{ contac.name }} {{ contac.lastName }}</td> <td>{{ contac.mail }}</td> <td> <a class="btn btn-primary" href="{{path('edit_contacts',{'idContact':contac.id })}}" role="button">Editar Contacto</a> </td> </tr> {% endfor %} </tbody> </table> </div>
{% endblock %}